{
  "name": "vocabulary-learner",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/vocabulary-learner/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/vocabulary-learner/browser",
        "index": "apps/vocabulary-learner/src/index.html",
        "main": "apps/vocabulary-learner/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/vocabulary-learner/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/vocabulary-learner/public"
          }
        ],
        "styles": [
          "@angular/material/prebuilt-themes/cyan-orange.css",
          "apps/vocabulary-learner/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2m",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "200kb",
              "maximumError": "500kb"
            }
          ],
          "outputHashing": "all",
          "baseHref": "/vocabulary-learner/"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "vocabulary-learner:build:production"
        },
        "development": {
          "buildTarget": "vocabulary-learner:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "vocabulary-learner:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/vocabulary-learner/jest.config.ts",
        "styles": [
          "node_modules/@angular/material/prebuilt-themes/cyan-orange.css"
        ]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "vocabulary-learner:build",
        "port": 4200,
        "spa": true
      }
    },
    "server": {
      "dependsOn": [
        "build"
      ],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/vocabulary-learner/server",
        "main": "apps/vocabulary-learner/src/server.ts",
        "tsConfig": "apps/vocabulary-learner/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "vocabulary-learner:build:development",
          "serverTarget": "vocabulary-learner:server:development"
        },
        "production": {
          "browserTarget": "vocabulary-learner:build:production",
          "serverTarget": "vocabulary-learner:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": [
          "/"
        ]
      },
      "configurations": {
        "development": {
          "browserTarget": "vocabulary-learner:build:development",
          "serverTarget": "vocabulary-learner:server:development"
        },
        "production": {
          "browserTarget": "vocabulary-learner:build:production",
          "serverTarget": "vocabulary-learner:server:production"
        }
      },
      "defaultConfiguration": "production"
    }
  }
}